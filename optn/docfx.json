{
  "build": {
    "content": [
      {
        "files": [
          "**/*.yml"
        ],
        "dest": "optn"
      },
      {
        "files": [
          "**/*.md"
        ],
        "exclude": [
          "**/obj/**",
          "optn/**"
        ],
        "src": null,
        "dest": null
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "optn/**"
        ],
        "src": null,
        "dest": null
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {},
    "fileMetadata": {},
    "template": [
      "op.html"
    ],
    "dest": "optn"
  }
}